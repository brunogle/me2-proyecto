                "\t.MeshDensityY \"Medium\"",
                "\t.MeshLinesSubstrate \"1\"",
                "\t.Create",
                "End With",
                "",
                "'----------------------------------------------------------------------------",
                "",
                "Dim sDefineAt As String",
                "sDefineAt = \"1000\"",
                "Dim sDefineAtName As String",
                "sDefineAtName = \"1000\"",
                "Dim sDefineAtToken As String",
                "sDefineAtToken = \"f=\"",
